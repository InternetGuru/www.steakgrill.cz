<?xml version="1.0" encoding="utf-8"?>
<ProgressiveWebApp>
  <var id="shortName">Steakgrill</var>
  <var id="manifest" modifyonly="modifyonly">
{
  "name": "$name",
  "short_name": "$shortName",
  "display": "minimal-ui",
  "start_url": "$rootUrl",
  "theme_color": "$themeColor",
  "background_color": "#333",
  "icons": [
    {
      "src": "/files/icon/256x256.png",
      "sizes": "256x256",
      "type": "image/png"
    },
    {
      "src": "/files/icon/512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
  </var>
</ProgressiveWebApp>
